{
  "name": "student-records",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "3.5",
    "babel-preset-es2015": "^6.24.1",
    "node": "^10.7.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-relay": "^1.6.0",
    "sync-request": "^6.0.0"
  },
  "scripts": {
    "db:reset": "rake db:drop db:create db:migrate; rails db:seed",
