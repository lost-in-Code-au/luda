{
  "presets": [
    "es2015",
    "es2016",
    "react",
    "stage-0",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "relay",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    ["transform-class-properties", { "spec": true }]
  ]
}